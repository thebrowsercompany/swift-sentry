<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>company.thebrowser.sentry-native.crashpad</id>
        <version>0.0.0.0</version>
        <title>Sentry Native SDK (Crashpad)</title>
        <description>A NuGet package for the Crashpad version of sentry-native</description>
        <authors>The Browser Company</authors>
        <projectUrl>https://github.com/thebrowsercompany/swift-sentry</projectUrl>
        <repository type="git" url="https://github.com/thebrowsercompany/swift-sentry" branch="main"/>
    </metadata>
    <files>
        <!-- Header File-->
        <file src="$HEADERDIR$\include\sentry.h" target="include"/>
        <!-- Windows Files-->
        <file src="$WINDIR$\bin\crashpad_handler.exe" target="bin\win64"/>
        <file src="$WINDIR$\bin\crashpad_handler.pdb" target="debug\win64"/>
        <file src="$WINDIR$\bin\crashpad_wer.dll" target="bin\win64"/>
        <file src="$WINDIR$\bin\crashpad_wer.pdb" target="debug\win64"/>
        <file src="$WINDIR$\bin\sentry.dll" target="bin\win64"/>
        <file src="$WINDIR$\bin\sentry.pdb" target="debug\win64"/>
        <file src="$WINDIR$\lib\sentry.lib" target="lib\win64"/>
        <!-- Linux Files-->
        <file src="$LINUXDIR$\bin\crashpad_handler" target="bin\linux64"/>
        <file src="$LINUXDIR$\lib\libsentry.so" target="lib\linux64"/>
        <!-- macOS Files -->
        <file src="$MACOSDIR$\bin\crashpad_handler" target="bin\macOS"/>
        <file src="$MACOSDIR$\lib\libsentry.dylib" target="lib\macOS"/>
        <file src="$MACOSDIR$\lib\libsentry.dylib.dSYM" target="debug\macOS"/>
    </files>
</package>